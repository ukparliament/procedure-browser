<%= render :partial => 'procedure/intro', :object => @procedure %>

<%= list_item_count_sentence( 'work package', @result_count ) %>

<%= content_tag( 'p', link_to( 'RSS', @rss_url, :class => 'rss' ) ) if @rss_url %>

<% unless @work_packages.empty? -%>
	<%= render :partial => 'procedure_work_package/results_per_page_selector_current', :locals => { :page => @page, :results_per_page => @results_per_page } %>
	<ol>
		<%= render :partial => 'procedure_work_package/work_package', :collection => @work_packages, :locals => { 'work_packages' => @work_packages} %>
	</ol>
	
	<%= render :partial => 'procedure_work_package/pagination_current', :locals => { :page => @page, :results_per_page => @results_per_page, :result_count => @result_count } %>
<% end -%>

<%= render :partial => 'common/identifier', :object => @procedure %>