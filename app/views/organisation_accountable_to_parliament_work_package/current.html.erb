<%= render :partial => 'organisation_accountable_to_parliament/intro', :object => @organisation_accountable_to_parliament %>

<%= content_tag( 'p', 'Work packages currently before Parliament resulting from papers made available to Parliament by this organisation.' ) %>

<%= list_item_count_sentence( 'work package', @result_count ) %>

<%= render :partial => 'library_design/feeds/feeds' %>

<% unless @work_packages.empty? -%>
	<%= render :partial => 'organisation_accountable_to_parliament_work_package/results_per_page_selector_current', :locals => { :page => @page, :results_per_page => @results_per_page } %>
	<ol class="unnumbered">
		<%= render :partial => 'work_package/work_package', :collection => @work_packages, :locals => { 'work_packages' => @work_packages} %>
	</ol>
	<%= render :partial => 'organisation_accountable_to_parliament_work_package/pagination_current', :locals => { :page => @page, :results_per_page => @results_per_page, :result_count => @result_count } %>
<% end -%>

<%= render :partial => 'common/identifier', :object => @organisation_accountable_to_parliament %>